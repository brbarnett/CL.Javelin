    <StackPanel Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
                Orientation="Vertical">
        <Button Content="Test Button"
                Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteButtonStyle}"/>
        <ToggleButton Content="Test Toggle"
                      Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteToggleButtonStyle}"/>
        <ToggleSwitch OffContent="Off" OnContent="On"
                      Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteToggleSwitchStyle}"/>
        <CheckBox Content="Test"
                  Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteCheckBoxStyle}"/>
        <ComboBox SelectedIndex="1"
        	Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteComboBoxStyle}">
            <x:String>item a</x:String>
            <x:String>item b</x:String>
            <x:String>item c</x:String>
        </ComboBox>
        <DatePicker Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteDatePickerStyle}"/>
        <ListView Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteListViewStyle}">
            <x:String>lv item a</x:String>
            <x:String>lv item b</x:String>
            <x:String>lv item c</x:String>
        </ListView>
        <RadioButton Content="test"
                     Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteRadioButtonStyle}"/>
        <TextBox Text="test" 
