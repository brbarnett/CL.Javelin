        <CalendarDatePicker Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteCalendarDatePickerStyle}"/>
        <ListBox Margin="{StaticResource DefaultThickness}" Style="{StaticResource CoyoteListBoxStyle}">
            <x:String>lb item a</x:String>
            <x:String>lb item b</x:String>
            <x:String>lb item c</x:String>
        </ListBox>
